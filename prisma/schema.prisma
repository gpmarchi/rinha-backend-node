generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Person {
  id        String   @id @default(uuid())
  nickname  String   @unique @db.VarChar(32)
  name      String   @db.VarChar(100)
  birthdate DateTime

  @@map("persons")
}
